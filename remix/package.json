{
  "name": "remix-starter-stackery",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "postinstall": "tsc -b && find ./node_modules/* -mtime +10950 -exec touch {} \\;",
    "build": "remix build && tsc -b",
    "dev": "concurrently \"remix run\" \"tsc -b -w --preserveWatchOutput\" \"cross-env NODE_ENV=development nodemon -w app -w build -w data-build -e js,jsx,ts,tsx,md,mdx server.js\"",
    "start": "node server.js"
  },
  "dependencies": {
    "@m14t/remix-run-apigateway": "https://github.com/m14t/remix-run-apigateway",
    "@remix-run/cli": "^0.8.1",
    "@remix-run/data": "^0.8.1",
    "@remix-run/express": "^0.8.1",
    "@remix-run/react": "^0.8.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "lambda-warmer": "^1.2.1",
    "morgan": "^1.10.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.5",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": "12"
  }
}
